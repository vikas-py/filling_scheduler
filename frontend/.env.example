# Frontend Environment Variables Template
# Copy this to .env.development or .env.production and fill in values

# API Base URL (must include /api/v1 prefix, no trailing slash)
VITE_API_URL=http://localhost:8000/api/v1

# WebSocket URL (no trailing slash)
VITE_WS_URL=ws://localhost:8000

# Enable WebSocket real-time features (set to false if backend doesn't support WebSocket yet)
VITE_ENABLE_WEBSOCKET=true

# Optional: Enable debug logging
# VITE_DEBUG=true

# Optional: Enable analytics
# VITE_ANALYTICS_ID=your-analytics-id
